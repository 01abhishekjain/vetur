{
    "name": "vetur",
    "displayName": "vetur",
    "description": "Vue tooling for VSCode",
    "version": "0.0.1",
    "publisher": "octref",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "vue",
            "aliases": ["Vue", "vue"],
            "extensions": [".vue"],
            "configuration": "./language-configuration.json"
        }, {
            "id": "scss",
            "aliases": ["SCSS", "Scss", "scss"],
            "extensions": [".scss"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vue",
            "scopeName": "text.html.vue",
            "path": "./syntaxes/vue.tmLanguage"
        }, {
            "language": "scss",
            "scopeName": "source.scss",
            "path": "./syntaxes/scss.tmLanguage"
        }]
    }
}